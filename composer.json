{
    "name": "notadd/cloud/upyun",
    "description": "Notadd Extension for cloud.",
    "type": "notadd-extension",
    "keywords": ["notadd", "cloud", "upyun", "extension"],
    "homepage": "https://notadd.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Notadd",
            "email": "notadd@ibenchu.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Notadd\\Cloud\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "replace": {
        "guzzlehttp/guzzle": "*",
        "guzzlehttp/promises": "*",
        "guzzlehttp/psr7": "*",
        "psr/http-message": "*"
    },
    "require": {
        "php": ">=7.0",
        "upyun/sdk": "^3.0"
    },
    "require-dev": {
        "notadd/installers": "0.14.*",
        "notadd/testing": "0.4.*",
        "phpunit/phpunit": "~6.0"
    }
}
